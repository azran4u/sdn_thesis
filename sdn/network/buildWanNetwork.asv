function [ output_args ] = buildWanNetwork( input_args )

    s = [1 1 1 2 3];
    t = [2 3 4 3 4];
    bw = [6 6.5 7 11.5 17]';
    latency = [6 6.5 7 11.5 17]';
    jitter = [6 6.5 7 11.5 17]';
    
    EdgeTable = table([s' t'], bw, latency, jitter, 'VariableNames',{'EndNodes' 'Weight' 'Code'});

end

